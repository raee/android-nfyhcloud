<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape android:shape="rectangle">
            <gradient android:angle="0" android:endColor="#e5e5e5" android:startColor="#e5e5e5" />
        </shape></item>
    <item><shape android:shape="rectangle">
            <gradient android:angle="0" android:endColor="@color/white" android:startColor="@color/white" />

            <stroke android:width="0.2dp" android:color="#dcdcdc" />
        </shape></item>

</selector>