<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:yx="http://schemas.android.com/apk/res/com.yixin.nfyh.cloud"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:overScrollMode="never" >

    <LinearLayout
        android:id="@+id/ll_hm_healths"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center"
        android:orientation="vertical" >

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            android:id="@+id/health_type_sbp"
            yx:name="@string/health_type_sbp"
            style="@style/width_match"
            yx:background="@drawable/h_sbp" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            android:id="@+id/health_type_dbp"
            yx:name="@string/health_type_dbp"
            style="@style/width_match"
            yx:background="@drawable/h_dbp" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            android:id="@+id/health_type_maibo"
            yx:name="@string/health_type_maibo"
            style="@style/width_match"
            yx:background="@drawable/h_maibo" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            android:id="@+id/health_type_xueyang"
            yx:name="@string/health_type_xueyang"
            style="@style/width_match"
            yx:background="@drawable/h_xueyang" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            android:id="@+id/health_type_tiwen"
            yx:name="@string/health_type_tiwen"
            style="@style/width_match"
            yx:background="@drawable/h_tiwen" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            android:id="@+id/health_type_xuetang"
            yx:name="@string/health_type_xuetang"
            style="@style/width_match"
            yx:background="@drawable/h_xuetang" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            yx:name="@string/health_type_huxi"
            style="@style/width_match"
            android:visibility="gone"
            yx:background="@drawable/h_xueyang" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            yx:name="@string/health_type_ecg"
            style="@style/width_match"
            android:visibility="gone"
            yx:background="@drawable/h_tiwen" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>

        <com.yixin.nfyh.cloud.ui.CustomerHealthType
            yx:name="@string/health_type_yundong"
            style="@style/width_match"
            android:visibility="gone"
            yx:background="@drawable/h_xuetang" >
        </com.yixin.nfyh.cloud.ui.CustomerHealthType>
    </LinearLayout>

</ScrollView>